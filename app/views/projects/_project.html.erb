<a class="ui link card" href="<%=project_path(project)%>">
  <div class="content">
    <div class="header">
      <%= project.name %>
    </div>
    <div class="meta">
      <%= project.created_at %></td>
    </div>
    <div class="description">
      <%= project.summary %>
    </div>
  </div>

  <div class="extra content">
    <i class="github icon"></i>
    <%= project.git %>

